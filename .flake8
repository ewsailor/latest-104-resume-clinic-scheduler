[flake8]
max-line-length = 88
extend-ignore =
    E203, W503,
    D107, D203, D213, D406, D407, D413,
    D415,
    E501, F401, E402, F541, F841
docstring-convention = google
exclude =
    .git,
    __pycache__,
    .mypy_cache,
    .pytest_cache,
    .venv,
    alembic,
    scripts,
    tests,
    *.pyc
